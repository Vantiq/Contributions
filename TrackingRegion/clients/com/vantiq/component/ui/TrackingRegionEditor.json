  "name" : "com.vantiq.component.ui.TrackingRegionEditor",
  "options" : {
    "RELOCATE" : [ {
      "path" : "controllers",
      "type" : "js"
    }, {
      "path" : "views",
      "type" : "json"
    } ],
    "activeTab" : "edit",
    "badgeCount" : 0,
    "componentConfiguration" : {
      "configurationProperties" : [ {
        "defaultValue" : null,
        "description" : "The TrackingRegion object to be shown and edited in this editor.",
        "hidden" : true,
        "propertyLabel" : "trackingRegion",
        "propertyType" : 60,
        "type" : 1
      }, {
        "defaultValue" : false,
        "description" : "Hide the toolbar if you want to embed the component as a pure editor into your own Client.",
        "hidden" : false,
        "propertyLabel" : "hideToolbar",
        "propertyType" : 4,
        "type" : 1
      }, {
        "defaultValue" : "    configuration.isLoadingZones = true;\n    client.data.isRegionModified = false;\n    if (region) {\n        configuration.trackingRegion = region;\n    } else if (!configuration.trackingRegion) {\n        configuration.trackingRegion = {\n            name: \"NewTrackingRegion\"\n        };\n    }\n\n    client.data.displayingObject = {\n        name: configuration.trackingRegion.name,\n        boundary: \"\",\n        direction: \"\",\n        distance: \"\",\n        conversionSpecification: \"\",\n        conversionOnly: configuration.trackingRegion.conversionOnly\n    };\n    if (configuration.trackingRegion) {\n        // create displayingObject to show the JSON propertes as Strings in the editor.\n\n        if (configuration.trackingRegion.boundary) {\n            client.data.displayingObject.boundary = JSON.stringify(configuration.trackingRegion.boundary);\n        }\n        if (configuration.trackingRegion.distance) {\n            client.data.displayingObject.distance = JSON.stringify(configuration.trackingRegion.distance);\n        }\n        if (configuration.trackingRegion.direction) {\n            client.data.displayingObject.direction = JSON.stringify(configuration.trackingRegion.direction);\n        } \n        if (configuration.trackingRegion.conversionSpecification) {\n            client.data.displayingObject.conversionSpecification = JSON.stringify(configuration.trackingRegion.conversionSpecification);\n        }            \n    }\n    configuration.isLoadingZones = false;",
        "functionParameters" : [ "region" ],
        "hidden" : false,
        "propertyLabel" : "loadRegion",
        "propertyType" : 62,
        "type" : 1
      }, {
        "defaultValue" : "    if (configuration.validateRegion()) {\n        return configuration.trackingRegion;\n    } else {\n        return null;\n    }",
        "description" : "Return a valid trackingRegion object. or null if there is error in user input.",
        "hidden" : false,
        "propertyLabel" : "getRegion",
        "propertyType" : 62,
        "type" : 1
      }, {
        "defaultValue" : false,
        "hidden" : true,
        "propertyLabel" : "isLoadingZones",
        "propertyType" : 4,
        "type" : 1
      }, {
        "defaultValue" : "    var error = null;\n    configuration.trackingRegion.conversionOnly = client.data.displayingObject.conversionOnly;\n    \n    if (!client.data.displayingObject.name || ( client.data.displayingObject.name.trim().length == 0)) {\n        error = \"Name can not be empty.\";\n    }\n \n    if (error) {\n         client.errorDialog(error);\n    } else {\n        configuration.trackingRegion.name = client.data.displayingObject.name.trim();\n        try {\n            if (client.data.displayingObject.boundary) {\n                client.data.displayingObject.boundary = client.data.displayingObject.boundary.trim();\n                if (client.data.displayingObject.boundary) {\n                    configuration.trackingRegion.boundary = JSON.parse(client.data.displayingObject.boundary);\n                }\n            }\n        }catch(err) {\n            error = \"Error parsing boundary: \"+err;\n            client.errorDialog(error);\n        }\n        try {\n            if (client.data.displayingObject.direction) {\n                client.data.displayingObject.direction = client.data.displayingObject.direction.trim();\n                if (client.data.displayingObject.direction) {\n                    configuration.trackingRegion.direction = JSON.parse(client.data.displayingObject.direction);\n                }\n            }\n        }catch(err) {\n            error = \"Error parsing direction: \"+err;\n            client.errorDialog(error);\n        }\n        try {\n            if (client.data.displayingObject.distance) {\n                client.data.displayingObject.distance = client.data.displayingObject.distance.trim();\n                if (client.data.displayingObject.distance) {\n                    configuration.trackingRegion.distance = JSON.parse(client.data.displayingObject.distance);\n                }\n            }\n        }catch(err) {\n            error = \"Error parsing distance: \"+err;\n            client.errorDialog(error);\n        }\n        try {\n            if (client.data.displayingObject.conversionSpecification) {\n                client.data.displayingObject.conversionSpecification = client.data.displayingObject.conversionSpecification.trim();\n                if (client.data.displayingObject.conversionSpecification) {\n                    configuration.trackingRegion.conversionSpecification = JSON.parse(client.data.displayingObject.conversionSpecification);\n                }\n            }\n        }catch(err) {\n            error = \"Error parsing conversionSpecification: \"+err;\n            client.errorDialog(error);\n        }\n    }\n\n    if (error) {\n        return false;\n    } else {\n        return true;\n    }",
        "hidden" : true,
        "propertyLabel" : "validateRegion",
        "propertyType" : 62,
        "type" : 1
      } ],
      "originalWidgetName" : "TrackingRegionEditor"
